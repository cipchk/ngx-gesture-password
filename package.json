{
  "name": "ngx-gesture-password",
  "version": "7.0.0",
  "description": "A smart gesture password locker for angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipchk/ngx-gesture-password.git"
  },
  "keywords": [
    "ngx-gesture-password",
    "ng2-gesture-password",
    "gesture",
    "gesture password",
    "hand lock"
  ],
  "author": "cipchk <cipchk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipchk/ngx-gesture-password/issues"
  },
  "homepage": "https://github.com/cipchk/ngx-gesture-password#readme",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json 'lib/*/*.ts'",
    "test": "ng test --code-coverage --watch=false",
    "site:build": "ng build --prod --build-optimizer",
    "site:gh": "ng build --prod --build-optimizer --base-href /ngx-gesture-password/ && gh-pages -d ./dist",
    "build": "bash build.sh",
    "release": "npm run build && cd publish && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "ngx-toastr": "^9.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "^0.11.1",
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.1",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "codecov": "^3.0.0",
    "ng-packagr": "^4.4.5",
    "tsickle": "^0.34.0"
  }
}
